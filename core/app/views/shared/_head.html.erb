<title><%= title %></title>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
<%= raw stylesheet_tags %>
<%= csrf_meta_tag %>
<%= javascript_include_tag 'jquery-1.3.2.min', 'jquery.validate.min.js', 'rails', :cache => 'jquery-core' %>
<% unless I18n.locale.to_s[0..1] == 'en' %>
  <%= javascript_include_tag ['localization/messages_', I18n.locale.to_s[0..1], '.js'].join %>
<% end %>
<%= yield :head %>
<%= raw meta_data_tags %>
